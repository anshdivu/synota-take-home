  "license": "MIT",
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22"
  },
  "scripts": {
    "build": "rm -rf dist && yarn tsc",
    "start": "ts-node -r dotenv/config bin/www",
    "watch": "nodemon --watch \"src/**\" --watch \"bin/**\" --watch \".env\" --ext \"ts,json\" --exec \"yarn start\"",
    "start:prod": "node ./dist/bin/www"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
